# Function to find the minimum element in a sorted list
def find_minimum_element(sorted_list):
    # Check if the list is empty
    if not sorted_list:
        return None

    # Return the first element in the sorted list (minimum element)
    return sorted_list[0]


# Given sorted list of integers
sorted_list = [1, 3, 5, 7, 9, 11, 13]

# Call the function and store the result
minimum_element = find_minimum_element(sorted_list)

# Print the result
if minimum_element is not None:
    print(f"The minimum element in the list is: {minimum_element}")
else:
    print("The list is empty")
